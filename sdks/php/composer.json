{
  "name": "inmova/sdk",
  "description": "Official PHP SDK for Inmova PropTech API",
  "keywords": [
    "inmova",
    "proptech",
    "real-estate",
    "property-management",
    "api",
    "sdk"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Inmova App",
      "email": "developers@inmova.app"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Inmova\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inmova\\Tests\\": "tests/"
    }
  }
}
